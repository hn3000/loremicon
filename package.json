{
  "name": "loremicon",
  "version": "0.9.1",
  "description": "",
  "repository": {
    "url": "https://github.com/hn3000/loremicon",
    "type": "git"
  },
  "main": "index.js",
  "scripts": {
    "prestart": "tsc -p tsconfig.json",
    "start": "node out/index",
    "tsc": "tsc -p tsconfig.json",
    "prebuild": "tsc -p tsconfig.json",
    "build": "webpack -c webpack.config.json",
    "webpack": "webpack-dev-server -c webpack.dev.json",
    "predocker": "npm run test",
    "docker": "docker build . -t loremicon",
    "docker-run": "docker-compose up -d --build",
    "pretest": "tsc -p tsconfig.test.json",
    "test": "node out-test/test/all-tests",
    "cover": "nyc npm test"
  },
  "keywords": [
    "lorem image",
    "lorem icon",
    "loremicon",
    "http",
    "rest",
    "api"
  ],
  "author": "Harald Niesche",
  "license": "MIT",
  "dependencies": {
    "canvas": "^2.6.1",
    "chart.js": "^2.9.3",
    "express": "^4.17.1",
    "full-icu": "^1.3.0"
  },
  "devDependencies": {
    "@types/chart.js": "^2.9.19",
    "@types/express": "^4.17.6",
    "clean-webpack-plugin": "^3.0.0",
    "html-webpack-plugin": "^4.3.0",
    "nyc": "^15.0.1",
    "ts-loader": "^7.0.2",
    "tsunit.external": "^2.1.1",
    "typescript": "^3.8.3",
    "webpack": "^4.43.0",
    "webpack-cli": "^3.3.11",
    "webpack-dev-server": "^3.11.0"
  },
  "nyc": {
    "all": true,
    "excludeAfterRemap": false,
    "extension": [
      ".js",
      ".ts"
    ],
    "include": [
      "out-test/src/**",
      "src/*.ts"
    ],
    "exclude": [
      "test/**",
      "out-test/test/**"
    ],
    "reporter": [
      "text",
      "html",
      "lcov"
    ]
  }
}
