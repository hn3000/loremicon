{
  "name": "loremicon",
  "version": "0.9.1",
  "description": "",
  "main": "index.js",
  "scripts": {
    "prestart": "tsc -p tsconfig.json",
    "start": "node out/index",
    "tsc": "tsc -p tsconfig.json",
    "predocker": "tsc",
    "docker": "docker build . -t loremicon",
    "pretest": "tsc -p tsconfig.test.json",
    "test": "node out-test/test/all-tests",
    "cover": "nyc npm test"
  },
  "keywords": [
    "lorem image",
    "lorem icon",
    "loremicon",
    "http",
    "rest",
    "api"
  ],
  "author": "Harald Niesche",
  "license": "MIT",
  "dependencies": {
    "canvas": "^2.6.1",
    "chart.js": "^2.9.3",
    "express": "^4.17.1",
    "full-icu": "^1.3.0"
  },
  "devDependencies": {
    "@types/chart.js": "^2.9.19",
    "@types/express": "^4.17.6",
    "typescript": "^3.8.3",
    "nyc": "^15.0.1",
    "tsunit.external": "^2.1.1"
  },
  "nyc": {
    "all": true,
    "excludeAfterRemap": false,
    "extension": [
      ".js",
      ".ts"
    ],
    "include": [
      "out-test/src/**",
      "src/*.ts"
    ],
    "exclude": [
      "test/**",
      "out-test/test/**"
    ],
    "reporter": [
      "text",
      "html"
    ]
  }
}
